<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <filter>
        <filter-name>THFilter</filter-name>
        <filter-class>servlet.THFilter</filter-class>
    </filter>
    <filter-mapping>
        <filter-name>THFilter</filter-name>
        <servlet-name>SearchServlet</servlet-name>
        <dispatcher>REQUEST</dispatcher>
        <dispatcher>FORWARD</dispatcher>
    </filter-mapping>
    <servlet>
        <servlet-name>SearchServlet</servlet-name>
        <servlet-class>servlet.SearchServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LoginServlet</servlet-name>
        <servlet-class>servlet.LoginServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>TargetServlet</servlet-name>
        <servlet-class>servlet.TargetServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ChooseTargetServlet</servlet-name>
        <servlet-class>servlet.ChooseTargetServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>LoginServlet</servlet-name>
        <url-pattern>/Login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SearchServlet</servlet-name>
        <url-pattern>/Search</url-pattern>
        <url-pattern>/S</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>TargetServlet</servlet-name>
        <url-pattern>/Target</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ChooseTargetServlet</servlet-name>
        <url-pattern>/ChooseTarget</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
